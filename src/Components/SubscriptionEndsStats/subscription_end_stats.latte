<div class="col-md-6">
    <div class="panel panel-default">
        <div class="panel-heading">{_subscriptions.admin.subscription_end_stats.subscription_types}</div>
        <ul class="list-group">
            <li n:foreach="$typesCounts as $type" class="list-group-item">{$type['type']->name} {$type['type']|typeContent} <span class="badge">{$type['per']}%</span> <span class="badge">{$type['count']}</span></li>
        </ul>
    </div>
</div>

<div class="col-md-6">
    <div class="panel panel-default">
        <div class="panel-heading">{_subscriptions.admin.subscription_end_stats.content_access_types}</div>
        <ul class="list-group">
            <li n:foreach="$contents as $name => $values" class="list-group-item">{$name} <span class="badge">{$values['per']}%</span> <span class="badge">{$values['count']}</span></li>
        </ul>
    </div>
</div>
